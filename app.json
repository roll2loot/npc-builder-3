    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3940256099942544~3419835294",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "16411283-8157-46ba-8914-b16a4bb27d68"
      }
    }
  }
